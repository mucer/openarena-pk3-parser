{
  "name": "openarena-pk3-parser",
  "version": "0.1.0",
  "description": "This library can be used to parse a OpenArena PK3 files and folders",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc && ncp README.md dist/README.md && copy-pkg package.json dist/package.json --ignore devDependencies,scripts",
    "build:w": "tsc -w",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "test": "ts-mocha src/**/*.spec.ts"
  },
  "author": {
    "name": "Daniel Leib",
    "email": "mucer@mailbox.org"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mucer/openarena-pk3-parser.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/expect": "^1.20.3",
    "@types/fs-extra": "^5.0.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.7",
    "@types/pngjs": "^3.3.2",
    "@types/unzipper": "^0.9.1",
    "copy-pkg": "^1.0.0",
    "expect": "^23.6.0",
    "mocha": "^5.2.0",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.2",
    "ts-mocha": "^2.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "fs-extra": "^7.0.1",
    "openarena-bsp-parser": "^0.1.1",
    "pngjs": "^3.3.3",
    "tga": "^1.0.3",
    "unzipper": "^0.9.4"
  }
}
